{
    "name": "Project Development",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "augment.vscode-augment"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/usr/bin/python",
                "python.analysis.typeCheckingMode": "basic"
            }
        }
    }
}